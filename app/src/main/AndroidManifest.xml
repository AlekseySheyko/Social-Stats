<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aleksey.sheyko.staticdemo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".InstagramLoginActivity"
            android:label="@string/app_name"
            android:noHistory="true"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter android:label="@string/app_name" >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectActivity"
            android:label="@string/title_activity_account_type"
            android:parentActivityName=".MainActivity"
            android:theme="@android:style/Theme.Holo.Light.Dialog.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="aleksey.sheyko.staticdemo.MainActivity" />
        </activity>
        <meta-data
            android:name="com.crashlytics.ApiKey"
            android:value="143d23456e254b1c4cf31fba3722e9f640989752" />
    </application>

</manifest>
